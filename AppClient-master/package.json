{
  "name": "app-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all --parallel test:tape test:jest",
    "test:tape": "node_modules/.bin/babel-tape-runner src/**/*.spec.js | node_modules/.bin/tap-spec",
    "test:jest": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "webpack": "node_modules/.bin/webpack",
    "webpack:production": "CIRCLECI=1 node_modules/.bin/webpack",
    "webpack-eslint": "node_modules/.bin/webpack --env.eslint=1",
    "mock": "node_modules/.bin/webpack --env.design=1",
    "dev-eslint": "node_modules/.bin/webpack-dev-server --env.eslint=1",
    "design": "node_modules/.bin/webpack-dev-server --env.design=1",
    "eslint": "node node_modules/eslint/bin/eslint.js --ignore-path .eslintignore -c .eslintrc.yml ./",
    "sass-lint": "sass-lint -v -q --max-warnings=0",
    "storybook": "start-storybook -s ./src/static -p 6006",
    "storybook:build": "branch=$(git symbolic-ref --short HEAD) && build-storybook -c .storybook -o `echo \".storybook_static/$branch\"` &&  branch=$branch sh ./storybook.deploy.sh",
    "precommit": "npm-run-all --parallel eslint sass-lint test"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "rootDir": "",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg|png|jpeg|jpg|mp3)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "setupTestFrameworkScriptFile": "<rootDir>/jestSetup.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.jsx?$"
  },
  "devDependencies": {
